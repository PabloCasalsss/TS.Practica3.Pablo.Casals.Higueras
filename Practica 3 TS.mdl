{UTF-8}
Deposito Baterias= INTEG (Entrada Baterias-"Perdida/Fuga"-Procesamiento,
		0)
	~	Dmnl
	~		|

EficienciaBase=0.8
	~	Dmnl
	~		|

EficienciaNeta=IF THEN ELSE (Temperatura>30, EficienciaBase- 0.04 , EficienciaBase )
	~	Dmnl
	~		|

Entrada Baterias=8500
	~	Dmnl
	~		|

ExtraccionMetales=Procesamiento*EficienciaNeta
	~	Dmnl
	~		|

Material Recuperado= INTEG (ExtraccionMetales-Venta Fabricantes,
		0)
	~	Dmnl
	~		|

"Perdida/Fuga"=Deposito Baterias*TasaFuga
	~	Dmnl
	~		|

Procesamiento=TasaProcesamiento*Deposito Baterias
	~	Dmnl
	~		|

TasaFuga=0.35
	~	Dmnl
	~		|

TasaProcesamiento=0.725
	~	Dmnl
	~		|

TasaVenta=0.925
	~	Dmnl
	~		|

Temperatura=32
	~	Grados
	~		|

Venta Fabricantes=Material Recuperado*TasaVenta
	~	Dmnl
	~		|

